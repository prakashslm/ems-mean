{
  "name": "ems-mean",
  "version": "0.0.1",
  "description": "Employee Management System ",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "nodemon app.js --exec babel-node",
    "prebuild": "rm -rf public && mkdir public",
    "build": "webpack -v -c ./webpack.config.js",
    "lint": "eslint --no-ignore -f checkstyle .",
    "test": "mocha tests/**/*Tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prakashslm/ems-mean.git"
  },
  "keywords": [
    "MEAN"
  ],
  "author": "Prakash",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prakashslm/ems-mean/issues"
  },
  "homepage": "https://github.com/prakashslm/ems-mean#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.9.25"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^5.3.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "should": "^13.2.3",
    "sinon": "^9.0.3",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": "^8 || ^10"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": 2500,
    "env": {
      "NODE_ENV": "development",
      "ENV": "TEST",
      "PORT": 4000
    }
  }
}
